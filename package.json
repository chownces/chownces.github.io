  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-responsive-carousel": "^3.2.21",
    "react-scripts": "4.0.3",
    "react-scroll": "^1.8.4"
  },
  "scripts": {
